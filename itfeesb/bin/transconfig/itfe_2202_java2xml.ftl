			    <EcnomicSubjectCode>${var2.EcnomicSubjectCode}</EcnomicSubjectCode>
			    </#if>
			    <Amt>${var2.Amt}</Amt>
			    <AcctProp>${var2.AcctProp}</AcctProp>
			    </Detail2202>
			</#list>
		</Bill2202>
		</#list>
	</MSG>
