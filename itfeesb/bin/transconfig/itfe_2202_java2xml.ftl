			<BudgetType>${var1.BudgetType}</BudgetType>
			<TrimSign>${var1.TrimSign}</TrimSign>
			<OfYear>${var1.OfYear}</OfYear>
			<Amt>${var1.Amt}</Amt>
			<StatInfNum>${var1.StatInfNum}</StatInfNum>
