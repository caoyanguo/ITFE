	<jms:websphere-connector name="jmsConnector1"
		jndiInitialFactory="com.sun.jndi.fscontext.RefFSContextFactory"
		jndiProviderUrl="${mq.jndiProviderUrl}" connectionFactoryJndiName="${mq.connectionFactoryJndiName}"
		jndiDestinations="true" forceJndiDestinations="true" maxRedelivery="1"
		noLocal="true" createMultipleTransactedReceivers="true"
		persistentDelivery="true" disableTemporaryReplyToDestinations="true"
		cacheJmsSessions="false" specification="1.1">
	</jms:websphere-connector>
    -->
	
	<spring:bean id="JMSConnectFactory" class="com.ibm.mq.jms.MQXAConnectionFactory">
		<spring:property name="hostName" value="${mq.HOST_TIPS}" />
		<spring:property name="port" value="${mq.PORT_TIPS}" />
		<spring:property name="queueManager" value="${mq.QM_TIPS}" />
		<spring:property name="channel" value="SVRCONN" />
		<spring:property name="transportType" value="1" />
		<spring:property name="CCSID" value="819" />
	</spring:bean>
	<spring:bean id="JMSConnectFactory2" class="com.ibm.mq.jms.MQXAConnectionFactory">
		<spring:property name="hostName" value="${mq.HOST_MOF}" />
		<spring:property name="port" value="${mq.PORT_MOF}" />
		<spring:property name="queueManager" value="${mq.QM_MOF}" />
		<spring:property name="channel" value="SVRCONN" />
		<spring:property name="transportType" value="1" />
		<spring:property name="CCSID" value="819" />
	</spring:bean>
