	<!-- 批量报文定时发送 -->
	<jms:endpoint name="Dispatchjms" queue="${mq.TO.PBC.BATCH}"
		disableTemporaryReplyToDestinations="true" synchronous="false"
		connector-ref="jmsConnectorBatch" transformer-refs="java2xml casign sentXml2file uptsendlogfilepath string2bytes ojbect2JMS">
		<jms:transaction action="JOIN_IF_POSSIBLE" />
	</jms:endpoint>
	
