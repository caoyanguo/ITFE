		<spring:property name="transportType" value="1" />
		<spring:property name="CCSID" value="819" />
	</spring:bean>
	 -->
	<jms:websphere-connector name="jmsConnectorOnline" 
		cacheJmsSessions="false" numberOfConsumers="10" connectionFactory-ref="JMSConnectFactory"
		acknowledgementMode="AUTO_ACKNOWLEDGE" eagerConsumer="true" disableTemporaryReplyToDestinations="true"
		specification="1.1"  createMultipleTransactedReceivers="true">
	</jms:websphere-connector>
	
	<jms:websphere-connector name="jmsConnectorBatch" 
		cacheJmsSessions="false" numberOfConsumers="10" connectionFactory-ref="JMSConnectFactory"
		acknowledgementMode="AUTO_ACKNOWLEDGE" eagerConsumer="true" disableTemporaryReplyToDestinations="true"
		specification="1.1" persistentDelivery="true" createMultipleTransactedReceivers="true">
	</jms:websphere-connector>
	<jms:websphere-connector name="jmsConnector2" 
		cacheJmsSessions="false" numberOfConsumers="10" connectionFactory-ref="JMSConnectFactory2"
		acknowledgementMode="AUTO_ACKNOWLEDGE" eagerConsumer="true" disableTemporaryReplyToDestinations="true"
		specification="1.1" createMultipleTransactedReceivers="true">
	</jms:websphere-connector>
