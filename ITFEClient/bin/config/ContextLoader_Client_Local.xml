<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC  "-//SPRING//DTD BEAN//EN" "file:./spring-beans.dtd">
<beans default-lazy-init="true">
	<bean id="applicationContextLoader" class="com.cfcc.jaf.core.loader.ApplicationContextLoader">
		<property name="fileList">
			<list>

				<value>config/PropertyLoader.xml</value>
				<value>config/ServiceInvoker_Client.xml</value>
				<value>config/ITFEConfig.xml</value>
				<value>config/databaseconfig/dev/DatabaseSourceConfig.xml</value>
				
				<!--
				<value>config/busiconfig/TableNameConfig.xml</value>
					ITFE 的数据库配置
				-->
			    <value>config/databaseconfig/ormap/ItfeOrmap.xml</value>
				<!--
					
					ormapping配置
				-->
				<value>config/databaseconfig/TransactionConfig.xml</value>
				<value>config/Services.xml</value>
				<value>config/ManServices.xml</value>
				<!--
					事务配置信息
					
				-->
				

				
			</list>
		</property>
	</bean>
</beans>





